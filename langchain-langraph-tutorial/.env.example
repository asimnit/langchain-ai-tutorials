# Environment Variables
# Copy this file to .env and fill in your values

# Azure AI Foundry - Anthropic Claude (required for LLM)
AZURE_ANTHROPIC_ENDPOINT=https://your-resource-name.services.ai.azure.com/anthropic/
AZURE_ANTHROPIC_API_KEY=your-azure-api-key-here
AZURE_ANTHROPIC_MODEL=claude-opus-4-6

# Azure OpenAI - Embeddings (required for RAG)
AZURE_OPENAI_ENDPOINT=https://your-resource-name.cognitiveservices.azure.com/
AZURE_OPENAI_API_KEY=your-azure-openai-api-key-here
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding-3-small
AZURE_OPENAI_API_VERSION=2024-02-01

# Optional: for LangSmith tracing (recommended for debugging)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your-langchain-api-key
LANGCHAIN_PROJECT=langchain-tutorial
